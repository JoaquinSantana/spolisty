<div id="userplaylists">
  <ul class="row">
    <%= div_for(current_user.playlists) do |p| %>
      <li class="col-md-12 usrplay"> 
        <span class="pull-left">
          <%= p.name %>
        </span>

        <p class="pull-right">
            tracks
        </p>
        <% unless p.tracks.blank? %>
          <ul class='row ulliststyle'>
            <% p.tracks.each do |tr| %>
              <li class='col-md-12'>
                <%= tr.name %>
              </li>
            <% end %>
          </ul>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>